{
  "name": "msd/terra_base",
  "description": "Base files and configurations for any MSD application.",
  "type": "drupal-custom-module",
  "keywords": [
    "Drupal",
    "MSD"
  ],
  "homepage": "https://gitlab.juntadeandalucia.es/msd/terra_base",
  "license": "proprietary",
  "source": {
    "type": "git",
    "url": "https://gitlab.juntadeandalucia.es/msd/terra_base.git",
    "reference": "3.0.x"
  },
  "authors": [
    {
      "name": "Manuel Adan",
      "homepage": "https://www.drupal.org/user/516420",
      "role": "Maintainer"
    }
  ],
  "require": {
    "ext-zend-opcache": "*",
    "ext-redis": "*",
    "ext-zip": "*",
    "drupal/admin_toolbar": "^3.0.2",
    "drupal/better_exposed_filters": "^6.0",
    "drupal/config_rewrite": "1.5",
    "drupal/core": "~9.5.9 || ^10",
    "drupal/field_group": "^3.0",
    "drupal/mailsystem": "^4.0",
    "drupal/redis": "^1.3",
    "drupal/token": "^1.6",
    "drupal/ultimate_cron": "^2.0, !=2.0-alpha8"
  },
  "extra": {
    "enable-patching": true,
    "patches": {
      "drupal/config_rewrite": {
        "#3380052 Rewrite languaged config fails on installations with non-English default language": "https://www.drupal.org/files/issues/2023-08-08/config_rewrite-default_language_config_rewrite_fail-3380052-2.patch"
      }
    }
  },
  "support": {
    "issues": "https://redmine-attgap.juntadeandalucia.es",
    "source": "https://gitlab.juntadeandalucia.es/msd/terra_base"
  }
}