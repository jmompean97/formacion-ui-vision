{%
  set classes = [
    'field',
    'field--name-' ~ field_name|clean_class,
    'field--type-' ~ field_type|clean_class,
    'field--label-' ~ label_display,
    'list-link'
  ]
%}
{%
  set title_classes = [
    'field__label',
    label_display == 'visually_hidden' ? 'visually-hidden',
  ]
%}

{% if label_hidden %}
  {% if multiple %}
    <div{{ attributes.addClass(classes, 'field__items') }}>
      {% for item in items %}
        <div{{ item.attributes.addClass('field__item') }}>
          <i class="fas fa-caret-right icon-green"></i> 
          <a href="{{ item.content }}">{{ item.content }}</a>
        </div>
      {% endfor %}
    </div>
  {% else %}
    {% for item in items %}
      <div{{ attributes.addClass(classes, 'field__item') }}>
        <i class="fas fa-caret-right icon-green"></i> 
        <a href="{{ item.content }}">{{ item.content }}</a>
      </div>
    {% endfor %}
  {% endif %}
{% else %}
  <div{{ attributes.addClass(classes) }}>
    <h2{{ title_attributes.addClass(title_classes) }}>{{ label }}</h2>
    {% if multiple %}
      <div class='field__items'>
    {% endif %}
    {% for item in items %}
      <div{{ item.attributes.addClass('field__item') }}>
        <i class="fas fa-caret-right icon-green"></i> 
        <a href="{{ item.content['#url'].uri }}" alt="{{ item.content['#title'] }}">{{ item.content['#title'] }}</a>
      </div>
    {% endfor %}
    {% if multiple %}
      </div>
    {% endif %}
  </div>
{% endif %}
