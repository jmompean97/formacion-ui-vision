{%
  set classes = [
    'paragraph',
    'paragraph--type--' ~ paragraph.bundle|clean_class,
    view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
    not paragraph.isPublished() ? 'paragraph--unpublished',
    'list-link'
  ]
%}
{% block paragraph %}
  <div{{ attributes.addClass(classes) }}>
    {% block content %}
      <i class="fas fa-caret-right icon-green"></i> <a href="{{ content.field_url1.0 }}" title="{{ content.field_titulo.0 }}"> {{ content.field_titulo.0 }} </a>
    {% endblock content %}
  </div>
{% endblock paragraph %}