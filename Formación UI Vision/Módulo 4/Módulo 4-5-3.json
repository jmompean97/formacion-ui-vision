{
  "Name": "Módulo 4-5-2",
  "CreationDate": "2025-11-28",
  "Commands": [
    {
      "Command": "open",
      "Target": "https://for-pd-portal-jda.apps.paas-pro.junta-andalucia.es/dashboard",
      "Value": "",
      "Description": ""
    },
    {
      "Command": "storeXpathCount",
      "Target": "xpath=//h3[text()='Buscar contenido']/following-sibling::div//li",
      "Value": "TotalTiposContenido",
      "Description": "Cuenta todos los elementos de lista (<li>) dentro del panel 'Buscar contenido'."
    },
    {
      "Command": "store",
      "Target": "1",
      "Value": "i",
      "Description": ""
    },
    {
      "Command": "while",
      "Target": "${i} < ${TotalTiposContenido}",
      "Value": "",
      "Description": ""
    },
    {
      "Command": "storeText",
      "Target": "xpath=//div[h3[text()='Buscar contenido']]/div/ul/li[${i}]/a/span[@class='label']",
      "Value": "currentLabel",
      "Description": ""
    },
    {
      "Command": "echo",
      "Target": "Ítem ${i}: ${currentLabel}",
      "Value": "orange",
      "Description": ""
    },
    {
      "Command": "executeScript_Sandbox",
      "Target": "return Number(${i}) + 1;",
      "Value": "i",
      "Description": ""
    },
    {
      "Command": "end",
      "Target": "",
      "Value": "",
      "Description": ""
    },
    {
      "Command": "echo",
      "Target": "Macro finalizada.",
      "Value": "blue",
      "Description": ""
    }
  ]
}